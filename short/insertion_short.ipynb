   "source": [
    "# time comp O(n^2)\n",
    "def insertion_short(arr):\n",
    " for i in range(1, len(arr)):\n",
    "     j=i-1\n",
    "     key=arr[i]\n",
    "     while j>=0 and arr[j]> key:\n",
    "         arr[j+1]=arr[j]\n",
    "         j-=1\n",
    "     arr[j+1]=key\n",
    " return arr\n",
    "\n",
    "#driver code\n",
    "arr=[75,90,100,95,85,80]\n",
    "result=insertion_short(arr)\n",
    "print(result)"
   ]
  },
